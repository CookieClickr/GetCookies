elements: list = file.split(", ")

NOSW: chr = 'N'
y= 0
x= 0
#keepTrack: dict = {x: [y]}
#keepTrack = keepingup(x,  y, keepTrack)

for element in elements:
    direction = element[0]
    distance = int(element[1:])
    if NOSW == 'N':
        if direction == 'R':
            x = x + distance
            NOSW = 'O'
        else:
            x = x-distance
            NOSW = 'W'
    elif NOSW == 'W':
        if direction == 'R':
            y = y + distance
            NOSW = 'N'
        else:
            y = y - distance
            NOSW = 'S'
    elif NOSW == 'O':
        if direction == 'R':
            y = y - distance
            NOSW = 'S'
        else:
            y = y + distance
            NOSW = 'N'
    elif NOSW == 'S':
        if direction == 'R':
            x = x - distance
            NOSW = 'W'
        else:
            x = x + distance
            NOSW = 'O'

print(abs(x)+abs(y))
