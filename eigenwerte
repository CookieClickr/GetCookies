import numpy as np

def berechne_eigenwert(matrix):
    eigenwerte, _ = np.linalg.eig(matrix)
    return eigenwerte

# Beispielmatrix
matrix = np.array([[4, -2],
                   [1,  1]])

# Eigenwert berechnen
eigenwerte = berechne_eigenwert(matrix)

print("Die Eigenwerte der Matrix sind:", eigenwerte)
